<launch>
  <arg name="map_file" value="$(find lattice_planner)/maps/gallery_NEW2.yaml" />
  <param name="use_sim_time" value="true" />
  <param name="clock_node/dT" type="double" value="0.02" />
  <param name="simulator_node/show_dt" type="bool" value="False" />
  <param name="mission_file" type="string" value="/home/mirko/Devel/diplomski/ws/src/mission_publisher/yaml/simple.yaml" />
  <param name="parameters_file" type="string" value="/home/mirko/Devel/diplomski/ws/src/robot_model/learning_data/robot_001.yaml" />
  <rosparam param="robot_list"> ['mirko', 'slavko'] </rosparam>
    
  <node pkg="rviz" type="rviz" name="rviz" />
    
  <include file="$(find robot_model)/launch/simple.launch">
    <arg name="robot_name" value="mirko" />
    <arg name="init_pose" value="'{position: {x: 2.0, y: 11.25, z: 0.0}, orientation: {x: 0.0,y: 0.0,z: -0.707106,w: 0.707106}}'" />
    <arg name="output" value="screen" />
    <arg name="map_file" value="$(arg map_file)" />
  </include>
  
  <include file="$(find robot_model)/launch/simple.launch">
    <arg name="robot_name" value="slavko" />
    <arg name="init_pose" value="'{position: {x: 11.25, y: 11.25, z: 0.0}, orientation: {x: 0.0,y: 0.0,z: -0.707106,w: 0.707106}}'" />
    <arg name="output" value="screen" />
    <arg name="map_file" value="$(arg map_file)" />
  </include>

  <node name="simulator_node" pkg="simulator" type="simulator_ideal.py" output="screen" />
  <node name="clock_node" pkg="simulator" type="clock_node.py" output="screen" />
  
  <node name="mission_publisher_node" pkg="mission_publisher" type="mission_publisher.py" />
  
</launch>
